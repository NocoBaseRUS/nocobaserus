  "engines": {
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^5.0.1",
    "@dnd-kit/sortable": "^6.0.0",
    "@emotion/css": "^11.7.1",
    "@formily/antd": "^2.0.7",
    "@formily/core": "^2.0.7",
    "@formily/react": "^2.0.7",
    "ahooks": "^3.0.5",
    "axios": "^0.24.0",
    "file-saver": "^2.0.5",
    "i18next": "^21.6.0",
    "marked": "^4.0.12",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.15.1",
    "react-image-lightbox": "^5.1.4",
    "use-deep-compare-effect": "^1.8.1"
  },
  "peerDependencies": {
    "@types/react": ">=16.8.0 || >=17.0.0",
    "@types/react-dom": ">=16.8.0 || >=17.0.0",
    "antd": "^4.18.5",
    "react": ">=16.8.0 || >=17.0.0",
    "react-dom": ">=16.8.0",
    "react-is": ">=16.8.0 || >=17.0.0",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
